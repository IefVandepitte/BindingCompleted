        <TextBlock x:FieldModifier="public" x:Name="page2PositionTextBlock" HorizontalAlignment="Left" Margin="10,64,0,0" Text="mesurement" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="0.081,-0.6" Width="124"/>
        <Button x:FieldModifier="public" x:Name="page2Button" Content="Inc
                " HorizontalAlignment="Left" Margin="194,70,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
